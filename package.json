{
  "name": "xfi.xapp.front",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "yarn  && next dev",
    "dev:https": "yarn  next dev --experimental-https -H 0.0.0.0",
    "dev:ts": "yarn  && yarn ts & yarn dev",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "build": "yarn  && next build",
    "build:analyze": "yarn  && ANALYZE=true next build",
    "start": "next start",
    "pretty": "prettier --write ./**/*.ts",
    "lint": "next lint . --no-cache",
    "lint:fix": "next lint . --fix --no-cache",
    "prepare": "husky install",
    "postinstall": "patch-package && cross-os postinstall",
    "patch:packages": "patch-package"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "sh postinstall.unix.sh",
      "linux": "sh postinstall.unix.sh",
      "win32": "echo '// TODO: Run `sh postinstall.unix.sh` command in bash terminal'"
    }
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/base": "5.0.0-beta.4",
    "@mui/material": "5.13.4",
    "@reduxjs/toolkit": "1.9.2",
    "axios": "1.7.7",
    "clsx": "1.2.1",
    "date-fns": "2.30.0",
    "ethers": "5.7.2",
    "formik": "2.4.6",
    "jotai": "^1.3.7",
    "lodash": "4.17.21",
    "ms": "^2.1.3",
    "ms.macro": "^2.0.0",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-intl": "6.4.2",
    "react-qr-code": "^2.0.11",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "reselect": "^5.1.1",
    "sass": "^1.63.6",
    "three": "^0.157.0",
    "typescript": "5.1.3",
    "typograf": "^7.1.0",
    "url-join": "^5.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@next/bundle-analyzer": "^14.2.5",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@typechain/ethers-v5": "^7.0.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "4.14.191",
    "@types/ms": "^0.7.31",
    "@types/ms.macro": "^2.0.1",
    "@types/node": "20.2.5",
    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-root-import": "6.6.0",
    "cross-os": "^1.5.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-tss-unused-classes": "0.0.4",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jest-transformer-svg": "^2.0.2",
    "lint-staged": "13.0.3",
    "patch-package": "^8.0.0",
    "prettier": "2.7.1",
    "ts-jest": "^29.1.2",
    "typechain": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "eslint --fix"
    ]
  },
  "volta": {
    "node": "18.17.0",
    "yarn": "1.19.2"
  }
}
